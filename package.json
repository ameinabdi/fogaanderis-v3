{
  "name": "fogaandersi",
  "version": "0.0.1",
  "description": "Description for Fogaandersi",
  "license": "UNLICENSED",
  "scripts": {
    "android": "expo run:android",
    "backend:start": "cd .. && npm run app:start",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo export",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "e2e:build:ios": "detox build --configuration ios.sim.release",
    "e2e:test:ios": "detox test --configuration ios.sim.release --retries 3",
    "postinstall": "patch-package",
    "ios": "expo run:ios",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "precommit": "lint-staged",
    "prepare": "husky",
    "prettier-check": "prettier --check \"{,**/}*.{md,json,yml,html,cjs,mjs,js,cts,mts,ts,tsx,css,scss,vue,java}\"",
    "prettier-format": "prettier --write \"{,**/}*.{md,json,yml,html,cjs,mjs,js,cts,mts,ts,tsx,css,scss,vue,java}\"",
    "publish": "expo publish",
    "start": "expo start",
    "test": "jest",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "web": "expo start --web"
  },
  "lint-staged": {
    "{,app/**/,test/**/}*.{js,json,yml,md,ts,tsx}": [
      "prettier --write"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "app/shared/services/api.js",
      "app/shared/themes/",
      "/storybook/",
      "/*\\.story\\.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "preset": "jest-expo",
    "setupFiles": [
      "./test/setup"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "testMatch": [
      "**/test/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/",
      "test/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native-community|@react-native|react-native|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|@storybook))"
    ]
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.0.4",
    "@kesha-antonov/react-native-background-downloader": "^3.2.6",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-masked-view/masked-view": "0.3.1",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/devtools": "6.0.27",
    "@react-navigation/drawer": "6.7.2",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/stack": "6.4.1",
    "@sayem314/react-native-keep-awake": "^1.3.0",
    "apisauce": "3.1.0",
    "expo": "51.0.38",
    "expo-constants": "~16.0.2",
    "expo-image-picker": "~15.0.5",
    "expo-linking": "~6.3.1",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "format-json": "1.0.3",
    "formik": "2.4.6",
    "identity-obj-proxy": "3.0.0",
    "lodash": "4.17.21",
    "lottie-react-native": "^7.2.2",
    "native-base": "^2.15.0",
    "querystringify": "2.2.0",
    "ramda": "0.30.1",
    "react": "18.2.0",
    "react-datepicker": "7.5.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-animatable": "^1.4.0",
    "react-native-animated-progress": "^1.0.2",
    "react-native-app-intro-slider": "^4.0.4",
    "react-native-collapsible": "^1.6.2",
    "react-native-easy-grid": "^0.2.2",
    "react-native-elements": "^3.1.0",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal-datetime-picker": "18.0.0",
    "react-native-music-control": "^1.4.1",
    "react-native-picker-select": "9.3.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-render-html": "^6.3.4",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-sectioned-multi-select": "0.10.0",
    "react-native-sticky-parallax-header": "^0.4.1",
    "react-native-track-player": "^4.1.1",
    "react-native-user-avatar": "^1.0.8",
    "react-native-vector-icons": "^10.2.0",
    "react-native-video": "^6.9.1",
    "react-native-web": "~0.19.13",
    "react-native-webview": "^13.13.1",
    "react-redux": "9.1.2",
    "redux": "5.0.1",
    "redux-persist": "6.0.0",
    "redux-saga": "1.3.0",
    "reduxsauce": "1.3.0",
    "seamless-immutable": "7.1.4",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/runtime": "7.26.0",
    "@config-plugins/detox": "8.0.0",
    "@react-native-community/cli-server-api": "15.0.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@redux-devtools/extension": "3.3.0",
    "@storybook/addons": "7.6.17",
    "@storybook/react-native": "7.6.19",
    "@storybook/theming": "8.1.6",
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-native": "12.8.0",
    "@types/jest": "29.5.14",
    "@types/react": "~18.2.79",
    "babel-jest": "29.7.0",
    "babel-preset-env": "1.7.0",
    "buffer": "6.0.3",
    "detox": "20.27.6",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "expect": "29.7.0",
    "generator-jhipster-react-native": "5.1.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "jest-expo": "~51.0.2",
    "lint-staged": "15.2.5",
    "mockery": "2.1.0",
    "patch-package": "8.0.0",
    "path-exists": "5.0.0",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.3",
    "react-test-renderer": "18.2.0",
    "resolve": "1.22.8",
    "rimraf": "5.0.7",
    "tapable": "2.2.1",
    "ts-jest": "29.2.5",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=6.x"
  }
}
